{
  "name": "ts-jest-tests",
  "version": "1.0.0",
  "description": "ts jest tests",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc",
    "test": "jest ./src/tests/**/*.test.ts --runInBand --forceExit --logHeapUsage --bail",
    "test:binery": "jest --testPathPattern=\".*.binery.test.ts\" --runInBand --forceExit --logHeapUsage --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apolonskiy/ts-jest-tests.git"
  },
  "author": "Andrii Polonskyi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apolonskiy/ts-jest-tests/issues"
  },
  "homepage": "https://github.com/apolonskiy/ts-jest-tests#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "typescript": "^4.5.2",
    "typescript-eslint": "0.0.1-alpha.0",
    "@typescript-eslint/parser": "^5.4.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "jwt-decode": "^3.1.2"
  }
}
